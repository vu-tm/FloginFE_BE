{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "lucide-react": "^0.548.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:unit": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:validationFrontend": "react-scripts test --coverage --watchAll=false --testPathPattern=loginValidation --collectCoverageFrom=src/utils/loginValidation.js",
    "test:loginIntegrationFrontend": "react-scripts test --coverage --watchAll=false --collectCoverageFrom=src/components/Login/Login.jsx --collectCoverageFrom=src/utils/loginValidation.js src/tests/integration/login.integration.test.js",
    "test:e2eFrontend": "cypress run --spec cypress/e2e/login.cy.js",
    "test:loginMockFrontend": "react-scripts test --coverage --watchAll=false --collectCoverageFrom=src/components/Login/Login.jsx src/tests/mock/login.mock.test.js",
  
    "test:e2eFrontendProduct": "cypress run --spec cypress/e2e/product.cy.js"
  
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-router-dom)/)"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "cypress": "^15.7.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jsx-a11y": "^6.5.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.3.2",
    "msw": "^2.12.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.7"
  }
}